{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "policy-briefs": {
      "root": "apps/policy-briefs/",
      "sourceRoot": "apps/policy-briefs/src",
      "projectType": "application",
      "prefix": "digital-first",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "sharepoint": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./apps/policy-briefs/custom-webpack.config.js",
              "mergeStrategies": { "externals": "replace" },
              "replaceDuplicatePlugins": true
            },
            "outputPath": "../sharepoint/deploy/SiteAssets/apps/policy-briefs",
            "index": "apps/policy-briefs/src/index.html",
            "main": "apps/policy-briefs/src/main.ts",
            "polyfills": "apps/policy-briefs/src/polyfills.ts",
            "tsConfig": "apps/policy-briefs/tsconfig.app.json",
            "assets": [
              "apps/policy-briefs/src/favicon.ico",
              "apps/policy-briefs/src/assets"
            ],
            "styles": ["apps/policy-briefs/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/policy-briefs/src/environments/environment.ts",
                  "with": "apps/policy-briefs/src/environments/environment.sharepoint.ts"
                }
              ],
              "deployUrl": "../../SiteAssets/apps/policy-briefs",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "sharepoint": {
              "fileReplacements": [
                {
                  "replace": "apps/policy-briefs/src/environments/environment.ts",
                  "with": "apps/policy-briefs/src/environments/environment.sharepoint.ts"
                }
              ],
              "deployUrl": "../../SiteAssets/apps/policy-briefs"
            }
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../sharepoint/deploy/SiteAssets/apps/policy-briefs",
            "index": "apps/policy-briefs/src/index.html",
            "main": "apps/policy-briefs/src/main.ts",
            "polyfills": "apps/policy-briefs/src/polyfills.ts",
            "tsConfig": "apps/policy-briefs/tsconfig.app.json",
            "assets": [
              "apps/policy-briefs/src/favicon.ico",
              "apps/policy-briefs/src/assets"
            ],
            "styles": ["apps/policy-briefs/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/policy-briefs/src/environments/environment.ts",
                  "with": "apps/policy-briefs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "sharepoint": {
              "fileReplacements": [
                {
                  "replace": "apps/policy-briefs/src/environments/environment.ts",
                  "with": "apps/policy-briefs/src/environments/environment.sharepoint.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4202,
            "browserTarget": "policy-briefs:build",
            "ssl": true,
            "sslKey": "ssl/server.key",
            "sslCert": "ssl/server.crt",
            "liveReload": true
          },
          "configurations": {
            "production": {
              "browserTarget": "policy-briefs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "policy-briefs:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/policy-briefs/jest.config.js",
            "tsConfig": "apps/policy-briefs/tsconfig.spec.json",
            "setupFile": "apps/policy-briefs/src/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/policy-briefs/tsconfig.app.json",
              "apps/policy-briefs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
