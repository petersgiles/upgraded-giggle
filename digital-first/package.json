    "sharepoint:copyListDefinitions": "cpx ../sharepoint/ListDefinitions/*.* ../sharepoint/deploy/ListDefinitions",
    "sharepoint:copyListData": "cpx ../sharepoint/ListDefinitions/*.* ../sharepoint/deploy/ListData",
    "sp:deploy": "run-s sharepoint:build sharepoint:copyListDefinitions sharepoint:copyListData deploy",
    "deploy": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ../sharepoint/deploy/PostDeploy.ps1 -SiteUrls 'http://vm-dev-lbs13/sites' -LoadReferenceData True",
